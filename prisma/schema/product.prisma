model Product {
    id String @id @default(uuid())
    name String
    price Float
    description String
    
    status Status @relation(fields: [statusId], references: [id])
    statusId Int

    owner Users? @relation(fields: [ownerId], references: [id])
    ownerId String

    order Order? @relation(fields: [orderId], references: [id])
    orderId String
}